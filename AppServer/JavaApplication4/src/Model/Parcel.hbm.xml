<?xml version="1.0"?><!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="Model.Parcel" table="parcel">
    
<composite-id name="parcelID">
<key-property name="parcelID" column="parcel_id" />
<key-property name="fromDate" column="from_date" />
</composite-id>


<property name="orderID" type="long" column="order_id" />
<property name="scanCode" type="long" column="scan_code" />
<property name="scanCodeType" type="string" column="scan_code_type" />
<property name="content" type="string" column="content" />
<property name="description" type="string" column="description" />
<property name="declaredValueCurrency" type="string" column="declared_value_currency" />
<property name="massKg" type="float" column="mass_kg" />
<property name="lenghtCm" type="float" column="lenght_cm" />
<property name="heightCm" type="float" column="height_cm" />
<property name="widthCm" type="float" column="width_cm" />
<property name="declaredValue" type="float" column="declared_value" />
<property name="timeStp" type="date" column="time_stp" />
<property name="toDate" type="date" column="to_date" />
</class>

<class name="Model.ParcelStatus" table="parcel_status">
<composite-id name="parcelID">
<key-property name="parcelID" column="parcel_id" />
<key-property name="fromDate" column="from_date" />
</composite-id>
<property name="status" type="string" column="status" />
<property name="comment" type="string" column="comment" />
<property name="toDate" type="date" column="to_date" />
</class>

</hibernate-mapping>