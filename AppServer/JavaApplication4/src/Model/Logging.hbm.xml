<?xml version="1.0"?><!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="Model.Logging" table="logging">
<composite-id name="loggingID">
<key-property name="userID" column="user_id" />
<key-property name="timeStp" column="time_stp" />
</composite-id>
<property name="actionType" type="string" not-null="true" column="action_type" />
</property>
</class>
</hibernate-mapping>